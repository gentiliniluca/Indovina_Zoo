<h1>Listing test_questions</h1>

<table>
  <thead>
    <tr>
      <th>Test</th>
      <th>Question</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @test_questions.each do |test_question| %>
      <tr>
        <td><%= test_question.test_id %></td>
        <td><%= test_question.question_id %></td>
        <td><%= link_to 'Show', test_question %></td>
        <td><%= link_to 'Edit', edit_test_question_path(test_question) %></td>
        <td><%= link_to 'Destroy', test_question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test question', new_test_question_path %>
