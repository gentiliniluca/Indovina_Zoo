<h1>Listing tests</h1>

<div class="moduloDiv">

  <table class="tabella" >
  <thead>
    <tr>
      <th>Id</th>
      <th>Level</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td><%= test.id %></td>
        <td><%= test.level %></td>
        <td><%= link_to 'Show', test %></td>
        <td><%= link_to 'Edit', edit_test_path(test) %></td>
        <td><%= link_to 'Destroy', test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>

  <tr>
    <td>
      <%= link_to 'New Test', new_test_path %>
    </td>
  </tr>
  </tbody>
</table>

<br>




    </div>